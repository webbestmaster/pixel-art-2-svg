{
  "name": "pixel-art-2-svg",
  "version": "0.1.0",
  "description": "pixel art to svg converter",
  "keywords": [
    "pixel",
    "pixelart",
    "pixel-art",
    "pixel art",
    "converter",
    "svg",
    "blockchain"
  ],
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node",
    "build": "babel ./src -d ./dist",
    "check": "npm run eslint; npm run flow;",
    "check:fix": "npm run prettier; npm run eslint:fix; npm run flow;",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "flow": "flow",
    "flow-stop": "flow stop",
    "prettier": "prettier --write \"./**/*.js\"",
    "test": "mocha --require babel-core/register --require babel-polyfill"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webbestmaster/pixel-art-2-svg.git"
  },
  "author": "webbestmaster",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webbestmaster/pixel-art-2-svg/issues"
  },
  "homepage": "https://github.com/webbestmaster/pixel-art-2-svg#readme",
  "dependencies": {
    "get-pixels": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^5.2.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-lodash": "^2.7.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.3.12",
    "eslint-plugin-optimize-regex": "^1.1.5",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-sonarjs": "^0.1.0",
    "eslint-plugin-unicorn": "^4.0.3",
    "flow-bin": "^0.77.0",
    "flow-typed": "^2.5.1",
    "mocha": "^5.2.0",
    "prettier": "^1.13.7"
  }
}
